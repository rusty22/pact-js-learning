{
  "consumer": {
    "name": "GraphQLConsumer"
  },
  "interactions": [
    {
      "description": "a movies request",
      "request": {
        "body": {
          "operationName": "MoviesQuery",
          "query": "\n          query MoviesQuery {\n            movies {\n              id\n              name\n              year\n            }\n          }\n        ",
          "variables": {}
        },
        "headers": {
          "Content-Type": "application/json"
        },
        "matchingRules": {
          "$.body.query": {
            "match": "regex",
            "regex": "\\s*query\\s*MoviesQuery\\s*\\{\\s*movies\\s*\\{\\s*id\\s*name\\s*year\\s*\\}\\s*\\}\\s*"
          }
        },
        "method": "POST",
        "path": "/graphql"
      },
      "response": {
        "body": {
          "data": {
            "movies": [
              {
                "id": 1,
                "name": "My GraphQL movie",
                "year": 1999
              }
            ]
          }
        },
        "headers": {
          "Content-Type": "application/json; charset=utf-8"
        },
        "matchingRules": {
          "$.body.data.movies": {
            "match": "type",
            "min": 1
          }
        },
        "status": 200
      }
    }
  ],
  "metadata": {
    "pact-js": {
      "version": "12.1.0"
    },
    "pactRust": {
      "ffi": "0.4.9",
      "models": "1.1.11"
    },
    "pactSpecification": {
      "version": "2.0.0"
    }
  },
  "provider": {
    "name": "GraphQLProvider"
  }
}